{
  "name": "upload-s3-action",
  "description": "Upload directory to AWS S3",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/sproctor/upload-s3-action",
  "author": "Dong Keon Kim <amos@shallwefootball.com>",
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/upload-s3-artifact.ts && git add -f dist/",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:test\" \"npm:build\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "jest --testTimeout 10000"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/glob": "^0.1.0",
    "aws-sdk": "^2.1242.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^13.11.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.5",
    "glob": "^7.1.6",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  }
}
