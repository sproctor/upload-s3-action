{
  "name": "upload-s3-action",
  "description": "Upload directory to AWS S3",
  "version": "1.2.0",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/shallwefootball/upload-s3-action",
  "author": "Dong Keon Kim <amos@shallwefootball.com>",
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/upload-artifact.ts && git add -f dist/",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:test\" \"npm:build\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/glob": "^0.1.0",
    "aws-sdk": "^2.1242.0",
    "klaw-sync": "^6.0.0",
    "mime-types": "^2.1.35",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "eslint": "^7.4.0",
    "eslint-plugin-github": "^4.1.1",
    "glob": "^7.1.6",
    "prettier": "^2.7.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "singleQuote": true
  }
}
